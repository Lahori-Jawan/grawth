    "esm": "^3.2.25",
    "express": "^4.16.4",
    "express-graphql": "^0.9.0",
    "graphql": "^14.5.4",
    "graphql-subscriptions": "^1.1.0",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.5",
    "jsonwebtoken": "^8.5.1",
