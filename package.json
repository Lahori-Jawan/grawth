{
  "name": "grawth",
  "version": "1.0.0",
  "description": "Graphql Auth with jwt",
  "author": "Nasir Khan",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
